{
  "kind": "collectionType",
  "collectionName": "projets",
  "info": {
    "singularName": "projet",
    "pluralName": "projets",
    "displayName": "Projet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nom": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "nom"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "date": {
      "type": "date"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "caracteristiques": {
      "type": "text"
    },
    "medias": {
      "type": "component",
      "component": "medias.slider",
      "repeatable": true
    },
    "description": {
      "type": "blocks"
    },
    "credits": {
      "type": "blocks"
    },
    "lien": {
      "type": "string"
    },
    "lien_nom": {
      "type": "string"
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "projet_cache": {
      "type": "boolean"
    }
  }
}
