{
  "name": "trrsss",
  "type": "nodejs",
  "build": {
    "command": "bash scripts/deploy-infomaniak.sh",
    "output": "backend/dist"
  },
  "deploy": {
    "source": "backend/dist",
    "destination": "/"
  },
  "environment": {
    "NODE_ENV": "production"
  },
  "start": {
    "command": "npm start"
  },
  "install": {
    "command": "npm run postinstall"
  }
}
